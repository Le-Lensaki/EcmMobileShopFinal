
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/all.css" rel="stylesheet" />
    <link href="~/Content/Slick/slick.css" rel="stylesheet" />
    <link href="~/Content/owl.carousel.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="~/Content/Styles/main.css">

</head>
<body>
    <div class="super_container">
        <header class="header">
            @{
                Html.RenderAction("_Header","Home");
            }
        </header>


        @RenderBody()


    <footer class="footer">
        @{
            Html.RenderAction("_Footer", "Home");
        }
    </footer>
        <div class="copyright">
            @{
                Html.RenderPartial("_Copyright");
            }
        </div>
    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)


    <script src="~/Scripts/fontawesome/fontawesome.js" type="text/javascript"></script>
    <script src="~/Scripts/Slick/slick.js" type="text/javascript"></script>
    <script src="~/Scripts/owl.carousel.js" type="text/javascript"></script>
    <script src="~/Scripts/isotope.pkgd.min.js"></script>
    <script src="~/Scripts/jquery-ui.js"></script>
    <script src="~/Scripts/parallax.min.js"></script>
    <script src="~/Scripts/styles/shop_custom.js" type="text/javascript"></script>
    <script src="~/Scripts/styles/product_custom.js" type="text/javascript"></script>
    <script src="~/Scripts/styles/cart_custom.js"></script>
    <script src="~/Scripts/styles/custom.js" type="text/javascript"></script>
    

</body>
</html>
